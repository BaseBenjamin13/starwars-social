<link rel="stylesheet" type href="style.css"/>

<h1>Tv-shows</h1>
<div class="container">
    
    <% for(let i=0; i < tvShows.length; i++){ %>
        <div class="item container-sm " style="width: 25vw">
            <h1><%=tvShows[i].title %></h1>
            <img src="<%=tvShows[i].img %>" alt="" class="img-fluid img-thumbnail">
        </div>
    <% } %>     
 
</div>