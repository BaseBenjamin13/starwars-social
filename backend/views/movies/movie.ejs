<h1>Welcome</h1>

<div class="container">

        <div class="item container-sm" >
            <h1><%=movie.title %></h1>
            
            <img src="<%=movie.img %>" alt="" class=" img-fluid img-thumbnail ">
            
        </div>
        <div>
            <a href="<%=movie.links.buy %>">BUY HERE</a>
            <br>
            <a href="<%=movie.links.trailer %>">View Trailer</a>
        </div>
        <br>
        
        <div class="container">
            <% for(let i=0; i < movie.comments.length; i++){ %>
                <div>
                        <h1><%=movie.comments[i] %></h1>
                </div>
            <% } %>     
        </div>

</div>



