
<link rel="stylesheet" type href="style.css"/>

<h1>Games</h1>
<div class="container">
    
    <% for(let i=0; i < games.length; i++){ %>
        <div class="item container-sm" style="width: 25vw">
            <a href='/games/<%=games[i]._id %>/game'>
                <h1><%=games[i].title %></h1>
            </a>
            <img src="<%=games[i].img %>" alt="" class="img-fluid img-thumbnail">
        </div>
    <% } %>     
 
</div>